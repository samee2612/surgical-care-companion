# LiveKit Cloud credentials
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret
LIVEKIT_URL=wss://your-livekit-cloud-url

# SIP trunk configuration
SIP_TRUNK_ID=your_sip_trunk_id

# Deepgram API key for STT
DEEPGRAM_API_KEY=your_deepgram_api_key

# ElevenLabs API key for TTS
ELEVEN_API_KEY=your_elevenlabs_api_key

# Twilio configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890
TWILIO_VERIFIED_CALLER_ID=+1234567890
TWILIO_SIP_DOMAIN=your_twilio_sip_domain
TWILIO_SIP_USERNAME=your_twilio_sip_username
TWILIO_SIP_PASSWORD=your_twilio_sip_password

# Agent configuration
AGENT_NAME=surgical-care-assistant
TTS_LANGUAGE=en-US

# Optional: Converse API endpoint
CONVERSE_API_URL=http://localhost:8000/api/v1/chat/converse

